---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>Civic User Testing Group</title>
  <link href="{{ site.url }}"/>
  <link type="application/atom+xml" rel="self" href="{{site.url}}/updates/feed.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>{{ site.url }}/</id>
  <author>
    <name>Smart Chicago Collaborative</name>
    <email>info@smartchicagocollaborative.org</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>{{ site.url }}{{ post.url }}</id>
    <link type="text/html" rel="alternate" href="{{ site.url }}{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>{{ post.author_name }}</name>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>